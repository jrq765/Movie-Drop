{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "/m/:id",
      "destination": "https://moviedrop.framer.website/m/:id",
      "permanent": false
    }
  ]
}
