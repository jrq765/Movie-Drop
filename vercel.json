{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/m/:id",
      "destination": "https://moviedrop.framer.website/m/:id",
      "permanent": false
    }
  ]
}
